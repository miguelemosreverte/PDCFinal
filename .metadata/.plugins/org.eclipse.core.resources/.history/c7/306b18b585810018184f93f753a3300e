jQuery(document).ready(function() {
	console.log("listo JQuery")

});

var JHoteles = {
		getLocalidades: function(sel) {
			console.log(sel.value);
            $("#result").html("<p>Procesando...</p>");
			$.ajax({
				url: "./getLocalidades.jsp",
				type: "get",
				data: {"codigoProvincia":sel.value},
				dataType: "html",
				error: function(hr) {
					$("#message").html(hr.responseText);
					$("#result").empty();
				},
				success: function(html) {
					$("#result").html(html);
				}
			})
		},
		buscar : function() {
			//const validar = () => $("[name=provincias]") && true
			//console.log(validar())
			console.log($("[name=provincias]:selected"))
			console.log("pepe")
		}
		
}
